.header
	// background rgba(31,31,31,0.1)
	position fixed
	top 0
	width 100%
	height 60px  
	opacity 1
	z-index 9999
	transition all 0.4s
	padding 10px
	.logo
		float left
		width 30%
		height inherit
		display inline-block
		transition inherit		  
		img
			margin 0 40px
			height 60px
			transition inherit
	.nav
		float left
		vertical-align top
		margin 20px 0
		width 40%
		height 20px
		display inline-block
		text-align center  
		transition inherit
		a
			text-decoration none
			color white
			line-height 20px
			vertical-align middle 
			padding 0 10px
			display inline-block
			font-weight 700 
			opacity 0.85
			transition inherit
			font-size 14px 
			&:hover
				opacity 1
		.active
			color #4DD0E1
			opacity 1 
			transition inherit
			&:hover
				opacity 0.75
	.formArea
		width 30%
		float left
		height inherit 
		position relative
		form
			position absolute
			top 0
			bottom 0
			right 70px
			margin auto
			height 30px
			width 315px
			background transparent
			& *
				height inherit 
				float left
			select
				background rgba(255, 255, 255, 0.5) 
				border solid 2px rgba(255, 255, 255, 0.0980392)
				color #333 
				font-weight 500
				text-transform uppercase 
				outline none
			option
				background #eee
			input
				box-sizing border-box
				outline none
				width 200px
				text-align center
				transition padding 0.4s
				background rgba(255, 255, 255, 0.0980392)
				border solid 1px rgba(255, 255, 255, 0.0980392)
				border-right none
				border-left none
				color white
				&:focus
					padding-left 10px
					text-align left
					transition padding 0.4s
			button
				background rgba(255, 255, 255, 0.5) 
				border solid 2px rgba(255, 255, 255, 0.0980392)
				border-left none
				color #333 
				font-weight 700
				text-transform uppercase 
				outline none
				padding 0 10px			
.landing
	width 100% 
	height 600px
	position fixed
	opacity 0.85
	overflow hidden
	.brandingArea
		position absolute
		top 0
		bottom 0
		left 0
		right 0
		margin auto
		height 220px
		text-align center
		.logo
			height 80px
			h1
				line-height 80px
				color white
				font-size 60px
				text-transform uppercase
		.description
			height 90px
			p
				color white
				font-size 18px
				line-height 60px
		.btn
			padding 20px 40px
			background #4DD0E1 
			font-size 16px
			font-weight 500
			color #FFF  
			text-transform uppercase
			letter-spacing 2px 
			-webkit-transition all 0.4s
			-moz-transition all 0.4s
			-ms-transition all 0.4s
			-o-transition all 0.4s
			transition all 0.4s
			border 1px solid transparent
			&:hover
				border 5px solid rgba(255,255,255,0.3)
				-webkit-transition all 0.4s
				-moz-transition all 0.4s
				-ms-transition all 0.4s
				-o-transition all 0.4s
				transition all 0.4s
	.carousel
		width 100%
		height inherit 
		ul
			height inherit
			transition margin-left 0.6s, margin-right 0.6s
		.slide1, .slide2, .slide3
			-webkit-background-size cover 
			-o-background-size cover 
			background-size cover 
			height inherit
			width 1080px  
			display inline-block
			float left 
			list-style none
			text-align center 
		.slide1
			background-image url("/img/girl2.jpg") 
		.slide2
			background-image url("/img/girl3.jpg") 
		.slide3
			background-image url("/img/girl.jpg") 
.leftArrow, .rightArrow
	cursor pointer
	position absolute
	height 120px
	width 120px
	top 0
	bottom 0 
	background-image url(/img/arrow.png) 
	-webkit-background-size contain 
	-o-background-size contain 
	background-size contain 
	margin auto
	opacity 0.2
	-webkit-transition all 0.8s
	-moz-transition all 0.8s
	-ms-transition all 0.8s
	-o-transition all 0.8s
	transition all 0.8s
	&:hover
		opacity 0.55
.leftArrow
	-webkit-transform scaleX(-1) 
	-moz-transform scaleX(-1) 
	-ms-transform scaleX(-1) 
	-o-transform scaleX(-1) 
	transform scaleX(-1) 
	left 20px
.rightArrow
	right 20px 
	
.girls
	width 100%
	position relative
	z-index 10
	background #333
	.image
		float left 
		width 25%
		overflow hidden 
		height 600px
		position relative
		img
			height 120%
			position absolute
			margin auto
			left -50%
			right -50%
			opacity 0.8
			margin-top 0
			transition all 0.4s ease
			transition-delay 0.1s
			&:hover
				margin-top -5%
				height 125%
				opacity 1
.shrink
	height 40px
	background #1F1F1F
	transition all 0.4s
	.logo
		img
			height 40px
			transition all 0.4s
	.nav
		margin 10px 0
		transition inherit
		a
			font-weight 500
.footer
	clear both 
	position relative
	z-index 10
	width 100% 
	background #252525
	padding-top 20px 
	.craps
		max-width 1080px
		height 160px 
		margin 0 auto
		.crap
			width 33%
			float left
			h3
				color #777
				font-weight 500
				font-size 18px 
				padding-bottom 10px 
			p
				color #ccc
				font-weight 300 
				font-size 16px 
				line-height 22px 
	.divisor
		border-top solid 1px rgba(255, 255, 255, 0.0980392)
		width 100% 
		.copy
			height 60px
			max-width 1080px
			margin 0 auto
			h3
				line-height 60px
				color #777
				font-weight 300
				font-size 14px  
			 